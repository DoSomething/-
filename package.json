{
  "private": true,
  "scripts": {
    "start": "npm run clean && NODE_ENV=development webpack --watch",
    "build": "npm run clean && NODE_ENV=production webpack",
    "build:dev": "npm run clean && NODE_ENV=development webpack",
    "clean": "rm -rf public/dist",
    "test": "jest --watch",
    "test:ci": "jest",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --fix --ext .js resources/assets"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "babel": {
    "presets": [
      "@dosomething/babel-config"
    ]
  },
  "jest": {
    "testURL": "http://rogue.dev",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/resources/assets/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@dosomething/forge": "^6.7.10",
    "@dosomething/gateway": "^1.1.5",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "dosomething-modal": "^0.3.3",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "sinon": "^3.3.0"
  },
  "devDependencies": {
    "@dosomething/babel-config": "^2.0.1",
    "@dosomething/eslint-config": "^3.1.1",
    "@dosomething/webpack-config": "^3.0.0",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^21.0.2",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^2.0.0",
    "eslint-loader": "^1.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.2",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.8.1"
  }
}
